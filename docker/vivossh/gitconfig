[core]
	repositoryformatversion = 0
	filemode = false
	bare = false
	logallrefupdates = true
	symlinks = false
	ignorecase = true
[remote "origin"]
	url = git@gitlab.com:ywzsunny/ywzvivo.git
	fetch = +refs/heads/*:refs/remotes/origin/*
[remote "vivo"]
	# 桌面端使用下面http配置 - 首次登陆后会自动保存账号密码  如果没有自动保存 输入: git config (--global) credential.helper store
    url = http://172.16.35.61:9080/ywzsunny/ywzvivo.git 
	# url = ssh://git@mygitlab.com:ywzsunny/ywzvivo.git
	fetch = +refs/heads/*:refs/remotes/origin/*
[branch "master"]
	remote = origin
	merge = refs/heads/master
